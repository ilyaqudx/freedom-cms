<script type="text/javascript">

	function toSubmit(){
		
		var payPassword = $("#payPassword").val();
		if(!payPassword){
			alert("输入6位长度的密码");
			return false;
		}
		$.ajax({
			type:'POST',
			url:"/verifyPayPwd?payPassword="+payPassword,
			dataType:'json',
			success:function(data){
				if(data.code == 0){
					window.location.href = data.result;
				}else{
					alert(data.msg);
				}
			}
		});
		return false;
	}
</script>

