<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="BankAccount">
  
	<select id="getBankList"  resultType="com.iquizoo.manage.web.bank.BankAccount">
		SELECT * FROM Bank
	</select>
	
	<select id="getBankById" resultType="Bank" parameterType="Integer">
		SELECT * FROM Bank WHERE id=#{id}
	</select>
	
	<select id="getBankByName" resultType="Bank" parameterType="String">
		SELECT * FROM Bank WHERE name=#{name}
	</select>
</mapper>