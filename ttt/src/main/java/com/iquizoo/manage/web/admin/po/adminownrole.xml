<mapper namespace="Adminownrole">

	<select id="getAdminownroleList"  resultType="Adminownrole">
		SELECT * FROM Adminownrole
	</select>

	<select id="getAdminownroleByAdminId" resultType="Adminownrole" parameterType="Integer">
		SELECT * FROM Adminownrole WHERE adminId=#{id}
	</select>

	<insert id="addAdminownrole" parameterType="Adminownrole">
		INSERT INTO Adminownrole(
		adminId,
		roleId
		)VALUES(
		#{adminId},
		#{roleId}
		)
	</insert>

	<update id="updateAdminownrole" parameterType="Adminownrole">
		UPDATE Adminownrole
		SET 
		adminId = #{adminId},
		roleId = #{roleId}
		WHERE 
		adminId=#{adminId}
		and roleId=#{roleId}
		
	</update>

	<delete id="deleteAdminownroleByKey" parameterType="Adminownrole">
	    DELETE FROM Adminownrole 
		WHERE 
		adminId=#{adminId}
		and roleId=#{roleId}
	</delete>

	<delete id="deleteAdminownroleByAdminId" parameterType="Integer">
		DELETE FROM Adminownrole 
		WHERE 
		adminId=#{id}
	</delete>
</mapper>