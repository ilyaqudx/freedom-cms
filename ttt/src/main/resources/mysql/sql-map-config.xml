<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//iBATIS.com//DTD SQL Map Config 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="false" errorTracingEnabled="true" maxRequests="32"
		maxSessions="10" maxTransactions="5" useStatementNamespaces="true" />

	<!--JAVA基类映射 -->
	<typeAlias alias="Integer" type="java.lang.Integer" />
	<typeAlias alias="String" type="java.lang.String" />
	<typeAlias alias="Float" type="java.lang.Float" />
	<typeAlias alias="Double" type="java.lang.Double" />
	<typeAlias alias="Timestamp" type="java.sql.Timestamp" />
	<typeAlias alias="List" type="java.util.List" />
	<typeAlias alias="Map" type="java.util.Map"/>
	
	<!-- 管理员 -->
	<typeAlias alias="Admin" type="com.iquizoo.manage.web.admin.po.Admin"/>
	<typeAlias alias="Resource" type="com.iquizoo.manage.web.admin.po.Resource"/>
	<sqlMap resource="com/iquizoo/manage/web/admin/po/admin.xml"/>
	<sqlMap resource="com/iquizoo/manage/web/admin/po/resource.xml"/>
	
	
</sqlMapConfig>